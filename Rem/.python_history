import dataloader
import numpy as np
import matplotlib.pyplot as plt
%matplotlib inline
for i in range(len(X_test)) :
    for j in range(0, i) :
        if (np.equal(X_test[i], X_test[j])):
            print X_test[i], X_test[j]
for i in range(len(X_test)) :
    for j in range(0, i) :
        if (np.equal(X_test[i, :], X_test[j, :])):
            print X_test[i], X_test[j]
for i in range(len(X_test)) :
    for j in range(0, i) :
        if (np.equal(X_test[i,], X_test[j,])):
            print X_test[i], X_test[j]
