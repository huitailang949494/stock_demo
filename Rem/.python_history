def Linear_SGD_Tester(prep = "z", eta=1e-4) :
    Linear_Regression = discriminative_classfier.LinearRegression(eta=eta, preprocessing=prep, max_epoch=500)
    Linear_Regression.train()
    return Linear_Regression.test()
